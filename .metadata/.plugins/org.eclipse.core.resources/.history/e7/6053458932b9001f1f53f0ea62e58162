package dugumolusturma;

public class Ana {
	public static void main(String[] args) {
		
		Sinif nesne1 = new Sinif(); // bellekte dügüm olusturduk.
		Sinif nesne2 = new Sinif();
		Sinif nesne3 = new Sinif();
		
		nesne1.sayi = 11; // dügümlere degerlerini atıyoruz.
		nesne2.sayi = 22;
		nesne3.sayi = 33;
		
		System.out.println(nesne1.sayi);
		System.out.println(nesne2.sayi);
		System.out.println(nesne3.sayi);
	}
}
